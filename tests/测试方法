curl -X GET "http://localhost:5001/api/v1/results" \
  -H "Content-Type: application/json"

# 获取最新的10条拨测结果
curl -X GET "http://localhost:5001/api/v1/results?page=1&size=1" \
  -H "Content-Type: application/json"

